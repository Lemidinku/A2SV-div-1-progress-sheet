from collections import defaultdict
class Solution:
    def subarraySum(self, nums: List[int], k: int) -> int:
        summ = 0
        table = {0:1}
        count = 0
        for num in nums:
            summ +=num
            if summ-k in table: count+= table[summ-k]
            if summ in table:table[summ] += 1
            else: table[summ]=1
        return count
